{
  "version": "1.1.0",
  "scripts": {
    "start": "react-scripts --openssl-legacy-provider start",
    "build": "react-scripts --openssl-legacy-provider build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "install:clean": "rm -rf node_modules/ && rm -rf package-lock.json && npm install && npm start",
    "compile-sass": "node-sass src/assets/scss/argon-dashboard-react.scss src/assets/css/argon-dashboard-react.css",
    "minify-sass": "node-sass src/assets/scss/argon-dashboard-react.scss src/assets/css/argon-dashboard-react.min.css --output-style compressed",
    "map-sass": "node-sass src/assets/scss/argon-dashboard-react.scss src/assets/css/argon-dashboard-react.css --source-map true",
    "start:server": "node bin/www",
    "watch:server": "npm run build && npm run start:server"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "dependencies": {
    "@aspnet/signalr": "^1.1.4",
    "@fortawesome/fontawesome-free": "5.12.1",
    "antd": "^4.6.0",
    "antd-img-crop": "^3.2.0",
    "axios": "^0.19.2",
    "chart.js": "2.9.3",
    "classnames": "2.2.6",
    "cookie-parser": "~1.4.4",
    "debug": "~2.6.9",
    "ejs": "~2.6.1",
    "express": "~4.16.1",
    "file-saver": "^2.0.2",
    "http-errors": "~1.6.3",
    "jwt-decode": "^2.2.0",
    "lodash": "^4.17.20",
    "moment": "^2.24.0",
    "morgan": "~1.9.1",
    "nouislider": "14.1.1",
    "react": "16.13.0",
    "react-chartjs-2": "2.9.0",
    "react-chat-widget": "^3.0.3",
    "react-copy-to-clipboard": "5.0.2",
    "react-datetime": "2.16.3",
    "react-dom": "16.13.0",
    "react-google-maps": "9.4.5",
    "react-helmet": "^6.1.0",
    "react-hook-form": "^5.7.2",
    "react-router-dom": "5.1.2",
    "react-scripts": "3.4.0",
    "react-treebeard": "^3.2.4",
    "reactstrap": "8.4.1",
    "reqwest": "^2.0.5",
    "sweetalert2-react": "^0.8.3",
    "treeview-react-bootstrap": "^0.4.6"
  },
